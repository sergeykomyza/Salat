footer.footer 
	figure.footer__logo 
		img(src="img/Logo-footer.png" alt="logo")
		figcaption Интернет магазин свежих
			br
			| и здоровых продуктов
	.footer__menus
		.footer__col
			h6.footer__title Заказать
			.footer__info
				p Москва и Московская область
				p 8 (495) 123-45-67
				p Ежедневно с 9:00 до 21:00
				p zakaz@salat.ru
		.footer__col.footer-menu
			h6.footer__title Каталог
			ul.footer-menu__list
				-
					var list = [
						"Фрукты", 
						"Овощи и зелень", 
						"Фермерские продукты",
						"Натуральные продукты",
						"Приправы и пряности",
						"Рыбы и морепродукты"
					]
				each item in list
					li.footer-menu__item
						img.footer-menu__icon(src="img/list-item-arrow.png" alt="arrow")
						a(href="/").footer-menu__link= item
		.footer__col.footer-menu
			h6.footer__title Полезное
			ul.footer-menu__list
				-
					var list = [
						"Готовые наборы", 
						"Подарочные корзины", 
						"Фрукты в офис",
						"Акции",
						"Программы лояльности"
					]
				each item in list
					li.footer-menu__item
						img.footer-menu__icon(src="img/list-item-arrow.png" alt="arrow")
						a(href="/").footer-menu__link= item
		.footer__col.footer-menu
			h6.footer__title Информация
			ul.footer-menu__list
				-
					var list = [
						"О магазине", 
						"Сотрудничество", 
						"Доставки и оплата",
						"Пользовательское соглашение",
						"Политика конфиденциальности",
						"Контакты"
					]
				each item in list
					li.footer-menu__item
						img.footer-menu__icon(src="img/list-item-arrow.png" alt="arrow")
						a(href="/").footer-menu__link= item
